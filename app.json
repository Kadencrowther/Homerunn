{
  "expo": {
    "name": "Homerunn",
    "slug": "homerunn",
    "owner": "kadencrowther",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/homerunnapp.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/homerunnapp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.homerunn.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to find properties near you.",
        "NSLocationAlwaysUsageDescription": "This app uses your location to find properties near you.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/homerunnapp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.homerunn.app",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/homerunnapp.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Homerunn to use your location to find properties near you."
        }
      ],
      "expo-font",
      "expo-apple-authentication",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "c9d6dcde-524c-4058-b8d0-2ba7c32d1219"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/c9d6dcde-524c-4058-b8d0-2ba7c32d1219"
    },
    "sdkVersion": "52.0.0"
  }
}
